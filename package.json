{
  "name": "skymatrix-server",
  "version": "1.1.3",
  "description": "e?",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "build-plugin": "tsc -p tsconfig-yunzai.json",
    "dev": "ts-node ./src/app.ts",
    "login": "ts-node ./src/login.ts",
    "update": "git fetch && git merge && npm install",
    "start": "tsc && pm2 start ./dist/app.js --name pu-client",
    "restart": "tsc && pm2 restart ./dist/app.js --name pu-client",
    "stop": "pm2 stop dist/app.js --name pu-client",
    "delete": "pm2 delete dist/app.js --name pu-client",
    "monitor": "pm2 monit dist/app.js --name pu-client",
    "logs": "pm2 logs --name pu-client"
  },
  "keywords": [],
  "author": "seiuna",
  "dependencies": {
    "chalk": "^4.1.2",
    "enquirer": "^2.4.1",
    "jose": "^5.6.3",
    "js-base64": "^3.7.7",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "koa": "2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-jwt": "^4.0.4",
    "koa-restql": "^15.0.0-beta",
    "koa-router": "^12.0.1",
    "koa-websocket": "^7.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "md5": "^2.3.0",
    "mongodb": "^6.8.0",
    "node-schedule": "^2.1.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa-websocket": "^5.0.10",
    "@types/lodash": "^4.14.202",
    "@types/md5": "^2.3.5",
    "@types/node-schedule": "^2.1.5",
    "@types/nodemailer": "^6.4.14",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
